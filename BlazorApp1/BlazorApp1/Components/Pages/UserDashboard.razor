@page "/userdashboard"
@inject NavigationManager Navigation

<div class="user-dashboard">

    <div class="left-panel">
        <h2>Hello, Tayyaba</h2>
        <p class="subtitle">Let's start living healthy from now on</p>


        <div class="chart-section">
            <img src="https://toneopeats-strapi-prod.s3.ap-south-1.amazonaws.com/chart_of_a_healthy_diet_02_b7a3be54bb.jpg" alt="Chart" style="width:300px; height:auto;" />
        </div>


        <h3>Meals today</h3>
        <div class="meals-grid">
            <div class="meal-card">
                <img src="https://cdn-icons-png.flaticon.com/512/8775/8775285.png" alt="Carrot Juice" />
                <div>
                    <h4>Carrot juice</h4>
                    <p>41kcal · 100g</p>
                </div>
            </div>

            <div class="meal-card">
                <img src="data:image/png;base64," alt="Mashed Potato" />
                <div>
                    <h4>Biryani</h4>
                    <p>180kcal · 200g</p>
                </div>
            </div>

            <div class="meal-card">
                <img src="https://img.freepik.com/premium-vector/paratha-food-vector-traditional_578838-41.jpg" alt="Corn Salad" />
                <div>
                    <h4>Pharata</h4>
                    <p>63kcal · 300g</p>
                </div>
            </div>

            <div class="meal-card">
                <img src="data:image/png;base64," alt="Tomato Juice" />
                <div>
                    <h4>Pizza</h4>
                    <p>17kcal · 100g</p>
                </div>
            </div>
        </div>
    </div>

    <div class="right-panel">
        <header class="top-bar">
            <button @onclick="() => Logout()">Logout</button>
            <i class="fas fa-bell"></i>
            <i class="fas fa-envelope"></i>
            <i class="fas fa-cog"></i>
        </header>

        <div class="calendar">
            <h3>November 2022</h3>
            <div class="calendar-grid">
                <span>19</span><span>20</span><span>21</span>
                <span class="active">22</span><span>23</span><span>24</span><span>25</span>
            </div>
        </div>

        <div class="report">
            <h3>Today report</h3>
            <div class="report-item">
                <i class="fas fa-utensils breakfast"></i> Breakfast - 280kcal
            </div>
            <div class="report-item">
                <i class="fas fa-pizza-slice lunch"></i> Lunch - 390kcal
            </div>
            <div class="report-item">
                <i class="fas fa-drumstick-bite dinner"></i> Dinner - 420kcal
            </div>
        </div>
    </div>
</div>

@code {
    private void Logout()
    {
        Navigation.NavigateTo("/login", true);
    }
}
<style>
    .user-dashboard {
        display: flex;
        gap: 30px;
        padding: 20px;
        font-family: Arial, sans-serif;
    }

    .left-panel {
        flex: 2;
    }

    .subtitle {
        color: #777;
        margin-bottom: 20px;
    }

    .chart-section {
        background: #fff;
        padding: 15px;
        border-radius: 12px;
        box-shadow: 0 2px 6px rgba(0,0,0,0.1);
        height: 250px;
        margin-bottom: 20px;
    }

    .meals-grid {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 15px;
    }

    .meal-card {
        display: flex;
        align-items: center;
        background: #fff;
        padding: 10px;
        border-radius: 12px;
        box-shadow: 0 2px 6px rgba(0,0,0,0.05);
        gap: 10px;
    }

    .meal-card img {
        width: 40px;
        height: 40px;
    }

    .right-panel {
        flex: 1;
        display: flex;
        flex-direction: column;
        gap: 20px;
    }

    .top-bar {
        display: flex;
        justify-content: flex-end;
        gap: 15px;
        font-size: 20px;
        color: #3b3bbf;
    }

    .calendar {
        background: #fff;
        padding: 15px;
        border-radius: 12px;
        box-shadow: 0 2px 6px rgba(0,0,0,0.05);
    }

    .calendar-grid {
        display: flex;
        gap: 10px;
        margin-top: 10px;
    }

    .calendar-grid span {
        padding: 8px 12px;
        border-radius: 8px;
        background: #f3f3f3;
        cursor: pointer;
    }

    .calendar-grid .active {
        background: #3b3bbf;
        color: #fff;
    }

    .report {
        background: #fff;
        padding: 15px;
        border-radius: 12px;
        box-shadow: 0 2px 6px rgba(0,0,0,0.05);
    }

    .report-item {
        padding: 8px 0;
        font-size: 14px;
    }

    .breakfast { color: #ff9800; }
    .lunch { color: #e64a19; }
    .dinner { color: #9c27b0; }

</style>